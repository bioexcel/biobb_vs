{
    "_id": "biobb_vs",
    "desc": "Biobb_vs is the Biobb module collection to perform virtual screening studies.",
    "github": "https://github.com/bioexcel/biobb_vs",
    "readthedocs": "https://biobb-vs.readthedocs.io/en/latest/",
    "conda": "https://anaconda.org/bioconda/biobb_vs",
    "docker": "https://quay.io/repository/biocontainers/biobb_vs",
    "singularity": "",
    "version": "3.0.0",
    "tools" : [
        {
            "block" : "AutoDockVina", 
            "tool" : "AutoDock Vina", 
            "desc" : "Docking of the ligand to a set of grids describing the target protein.",
            "exec" : "autodock_vina",
            "docs": "https://biobb-vs.readthedocs.io/en/latest/vina.html#module-vina.autodock_vina"
        },
        {
            "block" : "BindingSite", 
            "tool" : "in house using biopython", 
            "desc" : "Finds the binding site of the input_pdb file based on the ligands' location of similar structures (members of the sequence identity cluster)",
            "exec" : "bindingsite",
            "docs": "https://biobb-vs.readthedocs.io/en/latest/utils.html#module-utils.bindingsite"
        },
        {
            "block" : "Box", 
            "tool" : "in house using biopython", 
            "desc" : "Sets the center and the size of a rectangular parallelepiped box around a selection of residues found in a given PDB. The residue identifiers that compose the selection (i.e. binding site) are extracted from a second PDB.",
            "exec" : "box",
            "docs": "https://biobb-vs.readthedocs.io/en/latest/utils.html#module-utils.box"
        },
        {
            "block" : "ExtractModelPDBQT", 
            "tool" : "in house using biopython", 
            "desc" : "Extracts a model from a PDBQT file with several models.",
            "exec" : "extract_model_pdbqt",
            "docs": "https://biobb-vs.readthedocs.io/en/latest/utils.html#module-utils.extract_model_pdbqt"
        }
    ],
    "dep_pypi" : [
        "install_requires=['biobb_common==3.0.0', 'biopython==1.76']", 
        "python_requires='==3.7.*'"
    ], 
    "dep_conda" : [
        "python ==3.7.*", 
        "biobb_common ==3.0.0", 
        "biopython ==1.76",
        "autodock-vina ==1.1.2"
    ],
    "keywords" : [
        "virtual screening",
        "autodock",
        "vina"
    ]
}