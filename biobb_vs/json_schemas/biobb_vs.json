{
    "_id": "biobb_vs",
    "desc": "Biobb_vs is the Biobb module collection to perform virtual screening studies.",
    "github": "https://github.com/bioexcel/biobb_vs",
    "readthedocs": "https://biobb-vs.readthedocs.io/en/latest/",
    "conda": "https://anaconda.org/bioconda/biobb_vs",
    "docker": "https://quay.io/biocontainers/biobb_vs:3.7.0--py_0",
    "singularity": "",
    "version": "3.7.0",
    "tools" : [
        {
            "block" : "AutoDockVina", 
            "tool" : "AutoDock Vina", 
            "desc" : "Wrapper of the AutoDock Vina software.",
            "exec" : "autodock_vina",
            "docs": "https://biobb-vs.readthedocs.io/en/latest/vina.html#module-vina.autodock_vina"
        },
        {
            "block" : "BindingSite", 
            "tool" : "in house using biopython", 
            "desc" : "This class finds the binding site of the input_pdb.",
            "exec" : "bindingsite",
            "docs": "https://biobb-vs.readthedocs.io/en/latest/utils.html#module-utils.bindingsite"
        },
        {
            "block" : "Box", 
            "tool" : "in house using biopython", 
            "desc" : "This class sets the center and the size of a rectangular parallelepiped box around a selection of residues.",
            "exec" : "box",
            "docs": "https://biobb-vs.readthedocs.io/en/latest/utils.html#module-utils.box"
        },
        {
            "block" : "ExtractModelPDBQT", 
            "tool" : "in house using biopython", 
            "desc" : "Extracts a model from a PDBQT file with several models.",
            "exec" : "extract_model_pdbqt",
            "docs": "https://biobb-vs.readthedocs.io/en/latest/utils.html#module-utils.extract_model_pdbqt"
        },
        {
            "block" : "FPocket", 
            "tool" : "fpocket", 
            "desc" : "Wrapper of the fpocket software for finding the binding sites of a structure.",
            "exec" : "fpocket",
            "docs": "https://biobb-vs.readthedocs.io/en/latest/utils.html#module-fpocket.fpocket"
        },
        {
            "block" : "FPocketSelect", 
            "tool" : "fpocket_select", 
            "desc" : "Parses the outputs of the fpocket building block.",
            "exec" : "fpocket_select",
            "docs": "https://biobb-vs.readthedocs.io/en/latest/utils.html#module-fpocket.fpocket_select"
        }
    ],
    "dep_pypi" : [
        "install_requires=['biobb_common==3.5.1', 'biobb_structure_checking==3.7.3']", 
        "python_requires='==3.7.*'"
    ], 
    "dep_conda" : [
        "python ==3.7.*", 
        "biobb_common ==3.5.1", 
        "biobb_structure_checking==3.7.3",
        "autodock-vina ==1.1.2",
        "fpocket==3.1.4.2"
    ],
    "keywords" : [
        "virtual screening",
        "autodock",
        "vina",
        "fpocket"
    ]
}